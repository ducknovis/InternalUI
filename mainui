-- H√†m t·∫°o m√¥i tr∆∞·ªùng kh√¥ng gi·ªõi h·∫°n (gi·∫£ l·∫≠p)
local function createUnrestrictedEnvironment()
    local env = getfenv() -- L·∫•y m√¥i tr∆∞·ªùng hi·ªán t·∫°i
    -- Th√™m c√°c h√†m b·ªã ch·∫∑n trong Roblox (gi·∫£ l·∫≠p)
    env.require = function(id)
        return loadstring(game:HttpGet("https://raw.githubusercontent.com/some/repo/script.lua")) -- Gi·∫£ l·∫≠p require
    end
    env.getrawmetatable = function(t) return getmetatable(t) end -- B·ªè qua h·∫°n ch·∫ø metatable
    env.setfenv = setfenv or function(f, e) return f end -- H·ªó tr·ª£ setfenv
    env.getfenv = getfenv or function(f) return env end -- H·ªó tr·ª£ getfenv
    env.os = { time = os.time, date = os.date } -- Gi·∫£ l·∫≠p th∆∞ vi·ªán os
    env.debug = { traceback = debug.traceback } -- Gi·∫£ l·∫≠p debug
    return env
end

-- Logic n√∫t Execute c·∫£i ti·∫øn (kh√¥ng hi·ªÉn th·ªã tr·∫°ng th√°i)
executeButton.MouseButton1Click:Connect(function()
    local scriptText = textEditor.Text
    if scriptText and scriptText ~= "" then
        -- Ki·ªÉm tra c√∫ ph√°p v√† t·∫°o h√†m t·ª´ script
        local func, syntaxError = loadstring(scriptText)
        if not func then
            warn("Syntax Error: " .. syntaxError)
            return
        end

        -- T·∫°o m√¥i tr∆∞·ªùng kh√¥ng gi·ªõi h·∫°n
        local unrestrictedEnv = createUnrestrictedEnvironment()
        setfenv(func, unrestrictedEnv) -- G√°n m√¥i tr∆∞·ªùng m·ªõi cho h√†m

        -- Th·ª±c thi script trong m√¥i tr∆∞·ªùng kh√¥ng gi·ªõi h·∫°n
        local success, runtimeError = pcall(function()
            func() -- Ch·∫°y script
        end)
        if not success then
            warn("Runtime Error: " .. runtimeError)
        end
    end
end)

